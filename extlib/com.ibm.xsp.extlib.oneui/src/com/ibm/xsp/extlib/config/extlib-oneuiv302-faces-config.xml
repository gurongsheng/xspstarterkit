<faces-config>

  <render-kit>

    <renderer>
      <component-family>com.ibm.xsp.extlib.layout.ApplicationLayout</component-family>
      <renderer-type>com.ibm.xsp.extlib.layout.OneUIv302ApplicationLayout</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.layout.OneUIv302ApplicationLayoutRenderer</renderer-class>
    </renderer>
    
    <!-- Outline -->
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIv302BreadCrumbs</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.outline.OneUIv302BreadCrumbsRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIv302OutlineMenu</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.outline.OneUIv302OutlineMenuRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIv302DropDownButton</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.outline.OneUIv302DropDownButtonRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIv302LinksList</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.outline.OneUIv302LinksListRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIv302SortLinks</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.outline.OneUIv302SortLinksRenderer</renderer-class>
    </renderer>
    
    <!-- Containers -->
     <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.containers.OneUIv302WidgetContainer</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.containers.OneUIv302WidgetContainerRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.containers.OneUIv302InlineList</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.containers.OneUIv302InlineListRenderer</renderer-class>
    </renderer>
    
    <!-- Picker -->
    <renderer>
      <component-family>com.ibm.xsp.extlib.picker.Picker</component-family>
      <renderer-type>com.ibm.xsp.extlib.picker.OneUIv302ValuePicker</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.picker.OneUIv302ValuePickerRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.picker.Picker</component-family>
      <renderer-type>com.ibm.xsp.extlib.picker.OneUIv302NamePicker</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.picker.OneUIv302NamePickerRenderer</renderer-class>
    </renderer>
    
    <!-- Dialog -->
    <renderer>
      <component-family>com.ibm.xsp.extlib.Dialog</component-family>
      <renderer-type>com.ibm.xsp.extlib.dialog.OneUIv302Dialog</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.dialog.OneUIv302DialogRenderer</renderer-class>
    </renderer>
    
    <!-- Data -->
     <renderer>
      <component-family>com.ibm.xsp.extlib.data.ForumPost</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIv302ForumPost</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.data.OneUIv302ForumPostRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>com.ibm.xsp.extlib.data.FormLayout</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIv302FormTable</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.data.OneUIv302FormTableRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>com.ibm.xsp.DataIterator</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIv302ForumView</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.data.OneUIv302ForumViewRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIv302PagerSizes</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.data.OneUIv302PagerSizesRenderer</renderer-class>
    </renderer>
     <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIv302PagerDetail</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.data.OneUIv302PagerDetailRenderer</renderer-class>
    </renderer>
    <!-- Tagcloud -->
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.tagcloud.OneUIv302TagCloud</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.tagcloud.OneUIv302TagCloudRenderer</renderer-class>
    </renderer>
    <!-- deprecated - for compatibility only -->
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.OneUIv302TagCloud</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneuiv302.tagcloud.OneUIv302TagCloudRenderer</renderer-class>
    </renderer>
  </render-kit>
 
 

</faces-config>
