      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIForumPostRenderer</renderer-class>
    </renderer>

<!--
    Dialog
-->
    <renderer>
      <component-family>com.ibm.xsp.extlib.Dialog</component-family>
      <renderer-type>com.ibm.xsp.extlib.dialog.OneUIDialog</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dialog.OneUIDialogRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Dialog</component-family>
      <renderer-type>com.ibm.xsp.extlib.dialog.OneUIDialogButtonBar</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dialog.OneUIDialogButtonBarRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Dialog</component-family>
      <renderer-type>com.ibm.xsp.extlib.dialog.OneUIDialogContent</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dialog.OneUIDialogContentRenderer</renderer-class>
    </renderer>

<!--
    Dojo
-->
    <renderer>
      <component-family>javax.faces.Input</component-family>
      <renderer-type>com.ibm.xsp.extlib.dojoext.form.OneUINameTextBox</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dojoext.form.OneUIDojoExtNameTextBoxRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>javax.faces.Input</component-family>
      <renderer-type>com.ibm.xsp.extlib.dojoext.form.OneUIListTextBox</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dojoext.form.OneUIDojoExtListTextBoxRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>javax.faces.Input</component-family>
      <renderer-type>com.ibm.xsp.extlib.dojoext.form.OneUILinkSelect</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dojoext.form.OneUIDojoExtLinkSelectRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>javax.faces.Input</component-family>
      <renderer-type>com.ibm.xsp.extlib.dojoext.form.OneUIImageSelect</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.dojoext.form.OneUIDojoExtImageSelectRenderer</renderer-class>
    </renderer>

<!--
    Layout
-->
    <renderer>
      <component-family>com.ibm.xsp.extlib.layout.ApplicationLayout</component-family>
      <renderer-type>com.ibm.xsp.extlib.layout.OneUIApplicationLayout</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.layout.OneUIApplicationLayoutRenderer</renderer-class>
    </renderer>

<!--
    Outline
-->
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIOutlineMenu</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUIOutlineMenuRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIBreadCrumbs</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUIBreadCrumbsRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUISortLinks</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUISortLinksRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUILinksList</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUILinksListRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUISeparator</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUISeparatorRenderer</renderer-class>
    </renderer>
