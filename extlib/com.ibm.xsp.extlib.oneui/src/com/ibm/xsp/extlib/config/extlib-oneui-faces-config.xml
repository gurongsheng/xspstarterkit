      <component-family>com.ibm.xsp.extlib.Outline</component-family>
      <renderer-type>com.ibm.xsp.extlib.outline.OneUIDropDownButton</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.outline.OneUIDropDownButtonRenderer</renderer-class>
    </renderer>

<!--
    Picker
-->
    <renderer>
      <component-family>com.ibm.xsp.extlib.picker.Picker</component-family>
      <renderer-type>com.ibm.xsp.extlib.picker.OneUIValuePicker</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.picker.OneUIValuePickerRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.picker.Picker</component-family>
      <renderer-type>com.ibm.xsp.extlib.picker.OneUINamePicker</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.picker.OneUINamePickerRenderer</renderer-class>
    </renderer>

<!--
    Tagcloud
-->
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.tagcloud.OneUITagCloud</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.tagcloud.OneUITagCloudRenderer</renderer-class>
    </renderer>
    <!-- deprecated - for compatibility only -->
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.OneUITagCloud</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.tagcloud.OneUITagCloudRenderer</renderer-class>
    </renderer>


