      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUICustomViewRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.DataIterator</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIForumView</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIForumViewRenderer</renderer-class>
