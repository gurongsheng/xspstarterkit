<faces-config>

  <render-kit>

<!--
    Containers
-->
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.containers.OneUIWidgetContainer</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.containers.OneUIWidgetContainerRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>com.ibm.xsp.extlib.containers.OneUIInlineList</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.containers.OneUIInlineListRenderer</renderer-class>
    </renderer>

<!--
    Data
-->
    <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIPagerSizes</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIPagerSizesRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIPagerExpand</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIPagerExpandRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIPagerDetail</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIPagerDetailRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>com.ibm.xsp.extlib.data.Pager</component-family>
      <renderer-type>com.ibm.xsp.extlib.data.OneUIPagerAddRows</renderer-type>
      <renderer-class>com.ibm.xsp.extlib.renderkit.html_extended.oneui.data.OneUIPagerAddRowsRenderer</renderer-class>
