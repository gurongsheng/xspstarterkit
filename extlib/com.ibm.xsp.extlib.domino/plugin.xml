         <service type="com.ibm.xsp.extlib.controls.DynamicColumnBuilderFactory" class="com.ibm.xsp.extlib.component.dynamicview.DominoDynamicColumnBuilderFactory" />
   </extension>

   <extension point="com.ibm.commons.Extension">
